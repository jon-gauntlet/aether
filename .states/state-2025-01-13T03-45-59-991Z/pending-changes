+    onError: '#ffffff',
+    border: '#e5e7eb'
+  }
+};
+
+// Natural theme utilities
+export const getSpace = (theme: Theme) => theme.space;
+export const getBorderRadius = (theme: Theme) => theme.borderRadius;
+export const getColors = (theme: Theme) => theme.colors; 
