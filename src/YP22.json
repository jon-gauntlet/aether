{
    "name": "llama-7b-chat",
    "version": "2.0.0",
    "memory_requirements_gb": 4.0,
    "capabilities": [
        "chat",
        "completion",
        "instruction"
    ],
    "parameters": {
        "max_tokens": 2048,
        "temperature": 0.7,
        "top_p": 0.9,
        "frequency_penalty": 0.0,
        "presence_penalty": 0.0,
        "stop": ["</s>", "User:", "Assistant:"]
    },
    "context_size": 4096,
    "batch_size": 512,
    "threads": 4,
    "description": "LLaMA 7B chat model fine-tuned for conversation and instruction following",
    "license": "MIT",
    "source": "https://huggingface.co/models",
    "tags": [
        "llama",
        "chat",
        "instruction",
        "7b"
    ],
    "metrics": {
        "perplexity": 6.12,
        "training_loss": 1.89
    }
} 