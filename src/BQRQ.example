# Application
NODE_ENV=development
PORT=3000

# Docker
DOCKER_REGISTRY=your-registry
DOCKER_IMAGE=aether
DOCKER_TAG=latest

# Deployment
DEPLOY_HOST=your-host
DEPLOY_USER=your-user
DEPLOY_PORT=22

# Security
JWT_SECRET=your-jwt-secret
COOKIE_SECRET=your-cookie-secret

# API Keys (if needed)
# API_KEY=your-api-key

# Feature Flags
ENABLE_FLOW_PROTECTION=true
ENABLE_PATTERN_EVOLUTION=true
ENABLE_ENERGY_TRACKING=true

# Performance
MAX_WORKERS=4
CACHE_TTL=3600
BATCH_SIZE=100

# Monitoring
LOG_LEVEL=info
ENABLE_METRICS=true
METRICS_PORT=9090

# Development
VITE_DEV_SERVER_PORT=3000
VITE_HMR_PORT=3001
ENABLE_SOURCE_MAPS=true 