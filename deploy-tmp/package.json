{
  "name": "aether",
  "version": "0.1.0",
  "private": true,
  "description": "Natural digital consciousness and communication",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "ide:watch": "tsx watch src/systems/detection/ide.ts",
    "flow:monitor": "tsx watch src/systems/detection/flow.ts",
    "ai:prepare": "tsx src/ai/prepare.ts",
    "verify:env": "node scripts/verify-env.js",
    "dev:protected": "node scripts/verify-env.js && node scripts/monitor.js",
    "recovery": "node scripts/recovery.js",
    "monitor": "node scripts/monitor.js",
    "save:state": "node scripts/save-state.js",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "flow": "node scripts/flow.js",
    "flow:restore": "git checkout flow-backup && npm run flow",
    "flow:status": "node scripts/flow.js status",
    "flow:focus": "node scripts/flow.js focus",
    "flow:break": "node scripts/flow.js break",
    "build:watch": "vite build --watch",
    "type:watch": "tsc --watch --preserveWatchOutput",
    "protect": "node scripts/monitor.js",
    "protect:status": "node scripts/monitor.js status",
    "protect:integration": "tsx scripts/protect-integration.ts",
    "protect:recovery": "node scripts/recovery.js",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint",
    "validate:types": "tsx src/core/types/validation/validateTypes.ts",
    "precommit": "npm run validate:types && npm test",
    "analyze": "node scripts/analyze.js",
    "deploy": "node scripts/deploy-natural.js",
    "deploy:prepare": "npm run validate:types && npm run analyze && npm run build",
    "deploy:natural": "npm run deploy:prepare && npm run deploy",
    "deploy:flow": "npm run flow:status && npm run deploy:natural",
    "deploy:shield": "node scripts/monitor.js deploy",
    "deploy:restore": "node scripts/recovery.js deploy",
    "deploy:verify": "npx vercel --prod",
    "deploy:energy": "node scripts/analyze.js --energy-only",
    "deploy:patterns": "node scripts/analyze.js --patterns-only",
    "typecheck": "tsc --noEmit",
    "heal:types": "node scripts/heal-types.js",
    "heal:flow": "npm run heal:types && npm run deploy:flow"
  },
  "dependencies": {
    "@types/firebase": "^2.4.32",
    "firebase": "^11.1.0",
    "langchain": "^0.0.150",
    "openai": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "styled-components": "^6.0.7",
    "vscode-languageserver-protocol": "^3.17.3",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.37.3",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.0.3",
    "tsx": "^3.12.8",
    "typescript": "^5.7.3",
    "vite": "^4.4.9",
    "vitest": "^2.1.8"
  }
}
