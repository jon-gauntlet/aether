{
  "name": "aether",
  "version": "1.0.0",
  "description": "Aether Project",
  "type": "module",
  "scripts": {
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "frontend:test": "cd frontend && npm run test",
    "backend:dev": "./setup_env.sh poetry run uvicorn src.rag_aether.api.endpoints:app --reload --port 8000",
    "backend:start": "poetry run uvicorn src.rag_aether.api.endpoints:app --port 8000",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start": "concurrently \"npm run backend:start\" \"npm run frontend:preview\"",
    "test": "poetry run pytest && cd frontend && npm run test"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^3.4.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "concurrently": "^8.2.2",
    "framer-motion": "^11.18.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.64.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.0",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "prop-types": "^15.8.1",
    "react-router-dom": "^7.1.3",
    "react-syntax-highlighter": "^15.6.1",
    "styled-components": "^6.1.14",
    "vitest": "^3.0.2",
    "zod": "^3.24.1"
  }
}
