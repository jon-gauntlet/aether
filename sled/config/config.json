{
    "project": "aether",
    "stack": "python",
    "sled_version": "0.1.0",
    "paths": {
        "scripts": "sled/scripts",
        "docs": "sled/docs",
        "config": "sled/config"
    },
    "features": {
        "auto_init": true,
        "protection": true,
        "energy_tracking": true,
        "flow_state": true
    },
    "stack_config": {
        "python": {
            "use_poetry": true,
            "virtualenv_in_project": true,
            "pytest_enabled": true
        }
    },
    "protection": {
        "level": 2,
        "auto_recovery": true,
        "backup_interval": 3600
    },
    "energy": {
        "initial": 100,
        "low_threshold": 50,
        "critical_threshold": 20
    },
    "flow": {
        "check_interval": 300,
        "auto_restore": true
    }
} 